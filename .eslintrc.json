{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "globals": {
        "describe": "readonly",
        "it": "readonly",
        "before": "readonly",
        "after": "readonly",
        "beforeEach": "readonly",
        "afterEach": "readonly",
        "process": "readonly"

    },
    "rules": {
        "no-tabs": 0,
        "no-console": 0,
        "no-use-before-define": 0,
        "no-await-in-loop": 0,
        "indent": [
            "error",
            "tab"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "never"
        ]
    }
}
